<!--Export as Mobile-->
<extension 
       point="org.eclipse.ui.exportWizards"> 
      <wizard 
         id="zcstudio4.ExportZCMobileApp" 
         name="ZCMobile Application" 
         class="zcstudio4.exports.ExportZCMobileApp" 
         icon="icons/ZCIcon.png"> 
         <description> 
            A simple engine that exports ZCMobile project 
