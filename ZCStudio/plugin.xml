<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="preference.MyInitializer">
      </initializer>
   </extension>
<extension
point="org.eclipse.ui.preferencePages">

<page
id="pagepreference.ZeroCouplage"
name="ZeroCouplage"

class="preference.MainPreferencePage">
</page>
<page
id="pagepreference.ZeroCouplage.Desktop"
name="Desktop"
class="preference.DesktopPreferencePage"
category="pagepreference.ZeroCouplage">
</page>
<page
id="pagepreference.ZeroCouplage.Web"
name="Web"
class="preference.WebPreferencePage"
category="pagepreference.ZeroCouplage">
</page>
<page
id="pagepreference.ZeroCouplage.Mobile"
name="Mobile"
class="preference.MobilePreferencePage"
category="pagepreference.ZeroCouplage">
</page>
</extension>
<!--Run/ debug as Mobile-->
 <extension point="org.eclipse.debug.ui.launchShortcuts">
  <shortcut
    label="ZCMobile Application"
    icon="icons/ZCIcon.png"
    helpContextId="org.eclipse.jdt.debug.ui.shortcut_local_java_application"
    modes="run, debug"
    class="zcstudio4.shortcuts.zcmobileAction"
    description="Launch a local Java application"
    id="org.eclipse.jdt.debug.ui.localJavaShortcut">
    <description
     description="Runs a ZCMobile application"
     mode="run">
    </description>
    <description
     description="Debugs a ZCMobile application"
     mode="debug">
    </description>
  <contextualLaunch>
  <enablement>
    <with variable="selection">
     <count value="1"/>
      <iterate>
       <or>
        <test property="org.eclipse.jdt.launching.hasMain"/>
         <and>
          <test property="org.eclipse.jdt.launching.isContainer"/>
          <test property="org.eclipse.jdt.launching.hasProjectNature" args="org.eclipse.jdt.core.javanature"/>
         </and>
        </or>
      </iterate>
     </with>
  </enablement>
  </contextualLaunch>
  <configurationType
    id="org.eclipse.jdt.launching.localJavaApplication">
  </configurationType>
  </shortcut>
</extension>
